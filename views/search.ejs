<div class="author">
    <h1 class="author_heading">Search Results</h1>
</div>

<section class="posts">
    <ul class="posts_ul">
        <% data.forEach(post => { %>
            <li>
                <a href="/post/<%= post._id %>">
                    <span><%= post.title %></span>
                    <span class="posts_list__date"><%= post.createdAt.toDateString() %></span>
                </a>
            </li>
        <% }) %>
    </ul>
</section>